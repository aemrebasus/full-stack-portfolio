{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@shared/*": [
        "src/pma-app/shared/*"
      ],
      "@pma-services/*": [
        "src/pma-app/services/*"
      ],
      "@pma-entity-modules/*": [
        "src/pma-app/entities/*"
      ],
      "@pma-core/*": [
        "src/pma-app/core/*"
      ],
      "@pma/*": [
        "src/pma-app/*"
      ],
      "@style/*": [
        "src/pma-app/shared/style/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}